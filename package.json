{
  "name": "te",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.19.5",
    "axios": "^0.19.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-scripts": "5.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "update": "ncu -u && rm -rf node_modules && npm i",
    "build-css": "lessc src/assets/main.less ./src/assets/main.css --js",
    "watch-css": "less-watch-compiler src/ ./src --enable-js --main-file ./assets/main.less",
    "start": "npm run watch-css & react-scripts start",
    "build": "rm -rf build && npm run build-css && react-scripts build"
  },
  "devDependencies": {
    "less": "^3.9.0",
    "less-watch-compiler": "^1.13.0"
  },
  "browserslist": []
}
